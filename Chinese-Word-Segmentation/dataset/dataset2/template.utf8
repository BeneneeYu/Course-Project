# Unigram
U00:%x[-1,0]
U01:%x[0,0]
U02:%x[1,0]
U03:%x[-2,0]/%x[-1,0]
U04:%x[-1,0]/%x[0,0]
U05:%x[-1,0]/%x[1,0]
U06:%x[0,0]/%x[1,0]
U07:%x[1,0]/%x[2,0]

# Bigram
U00:%x[-1,0]
U01:%x[0,0]
U02:%x[1,0]
U03:%x[-2,0]/%x[-1,0]
U04:%x[-1,0]/%x[0,0]
U05:%x[-1,0]/%x[1,0]
U06:%x[0,0]/%x[1,0]
U07:%x[1,0]/%x[2,0]